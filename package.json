{
  "name": "blog_vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tar -cvzf build.tar.gz -C build .",
    "build:dev": "tsc && vite build --mode development",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:deploy": "cd deploy && go build -o ../bin deploy.go",
    "deploy": "npm run build && go run deploy/deploy.go"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.4.12",
    "@fancyapps/ui": "^5.0.17",
    "antd": "^5.4.6",
    "axios": "^1.4.0",
    "jquery": "^3.6.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "rc-menu": "^8.10.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown-editor-cm5": "^2.2.12",
    "react-router-dom": "^5.3.4",
    "react-slick": "^0.29.0",
    "react-transition-group": "^4.4.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/lodash": "^4.14.194",
    "@types/markdown-it-emoji": "^2.0.2",
    "@types/markdown-it-footnote": "^3.0.0",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.10",
    "@types/react-transition-group": "^4.4.5",
    "@types/uslug": "^1.0.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "less": "^4.1.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-cdn-import": "^0.3.5"
  }
